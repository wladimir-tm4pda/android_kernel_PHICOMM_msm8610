/dts-v1/;

/include/ "msm8610-v1.dtsi"
/include/ "msm8610-qrd-skuab.dtsi"
/include/ "msm8612-qrd-camera-sensor.dtsi"

/ {
	model = "Qualcomm MSM 8610v1 QRD C230v";
	compatible = "qcom,msm8610-qrd", "qcom,msm8610", "qcom,qrd";
	qcom,board-id = <0xc2300b 3>;
};

&mdss_mdp {
        qcom,mdss-pref-prim-intf = "dsi";
};

&mdss_dsi0 {
        qcom,dsi-pref-prim-pan = <&dsi_ili9806c_wvga_vid>;
};

&dsi_ili9806c_wvga_vid {
        qcom,cont-splash-enabled;
};

&dsi_fl10801a_wvga_vid {
        qcom,cont-splash-enabled;
};

&dsi_fx_nt35510_wvga_cmd {
        qcom,cont-splash-enabled;
};
